generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id        String   @id @default(uuid())
  data      Bytes
  mime      String
  name      String?
  createdAt DateTime @default(now())
}
